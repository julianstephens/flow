{
  "name": "api",
  "license": "MIT",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.ts",
  "scripts": {
    "watch:api": "yarn build; nodemon -e ts, json -x \"node --loader ts-node/esm ./src/app.ts\"",
    "debug:api": "yarn build; nodemon -e ts, json -x \"node --inspect=0.0.0.0:9229 --loader ts-node/esm ./src/app.ts\"",
    "watch:openapi": "nodemon -w src -e ts -x \"npx tsoa spec-and-routes\"",
    "build": "npx tsoa spec-and-routes; npx tsc --experimentalDecorators",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix --quiet --max-warnings=0; yarn code:format --write",
    "code:lint": "npx eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "npx prettier ./src/**/**.{js,jsx,ts,tsx}",
    "code:check:rules": "npx eslint-config-prettier .eslintrc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julianstephens/budget-tracker.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/julianstephens/budget-tracker/issues"
  },
  "homepage": "https://github.com/julianstephens/budget-tracker#readme",
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-jwt": "^6.0.4",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "nodemon": "^2.0.20",
    "poetic": "^1.7.0",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@types/lodash": "^4.14.185",
    "@types/morgan": "^1.9.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "helmet": "^6.0.0",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "redis": "^4.3.1",
    "redis-om": "^0.3.6",
    "tsoa": "^4.1.3",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "_moduleAliases": {
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@controllers": "dist/controllers"
  }
}
