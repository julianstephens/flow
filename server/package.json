{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "npx barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && nodemon -x \"node -r ts-node/register --inspect=0.0.0.0:9229 src/index.ts\"",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "npx eslint --ext .js,.ts \"src/\"",
    "code:format": "npx prettier './src/**/**.{js,ts}'",
    "code:check:rules": "npx eslint-config-prettier .eslintrc.js"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@tsed/ajv": "7.2.3",
