    "@tsed/cli-plugin-jest": "4.1.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.2.0",
