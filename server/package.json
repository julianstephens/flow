    "@tsed/di": "7.2.3",
    "@tsed/engines": "7.2.3",
    "@tsed/exceptions": "7.2.3",
    "@tsed/ioredis": "7.2.3",
    "@tsed/json-mapper": "7.2.3",
    "@tsed/logger": "6.3.2",
    "@tsed/logger-file": "6.3.2",
    "@tsed/passport": "7.2.3",
    "@tsed/platform-cache": "7.2.3",
    "@tsed/platform-exceptions": "7.2.3",
    "@tsed/platform-express": "7.2.3",
    "@tsed/platform-log-middleware": "7.2.3",
    "@tsed/platform-middlewares": "7.2.3",
    "@tsed/platform-params": "7.2.3",
    "@tsed/platform-response-filter": "7.2.3",
    "@tsed/platform-views": "7.2.3",
    "@tsed/prisma": "7.2.3",
    "@tsed/schema": "7.2.3",
    "@tsed/swagger": "7.2.3",
    "ajv": "^8.11.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-jwt": "^7.7.5",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "jwks-rsa": "^2.1.5",
