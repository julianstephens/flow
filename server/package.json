    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  },
  "prettier": "../.prettierrc.js",
  "prisma": {
    "seed": "npx ts-node --transpile-only ./prisma/seed.ts"
  }
