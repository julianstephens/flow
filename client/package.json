{
  "name": "client",
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "config": "ts-node-esm ./scripts/setenv.ts",
    "start": "yarn run config -- --environment=dev && ng serve --host 0.0.0.0 --disable-host-check",
    "build": "yarn run config -- --environment=prod && ng build",
    "test": "ng test",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:check:rules": "npx eslint-config-prettier .eslintrc.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/localize": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@auth0/auth0-angular": "^1.10.1",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@ng-select/ng-select": "^9.0.2",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "dotenv": "^16.0.2",
    "lodash": "^4.17.21",
    "rxjs": "~7.5.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.0",
    "@schuchard/prettier": "^5.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.5",
    "@types/yargs": "^17.0.13",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "poetic": "^1.7.0",
    "prettier": "latest",
    "tslint-config-prettier": "latest",
    "typescript": "~4.7.2"
  }
}
