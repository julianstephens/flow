{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env",
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "outputs": [
        "dist"
      ],
      "dependsOn": [
        "^db:generate"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^db:generate"
      ]
    },
    "lint": {},
    "format": {},
    "test": {
      "dependsOn": [
        "^db:generate"
      ]
    },
    "db:generate": {
      "cache": false,
      "dependsOn": [
        "^db:generate"
      ]
    },
    "db:migrate:deploy": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    }
  }
}
